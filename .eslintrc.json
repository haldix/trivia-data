{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-process-exit": "off",
    "no-confirm": "off",
    "no-alert": "off"
    // "func-names": "off",
    // "endOfLine": "auto",
    // "class-methods-use-this": "off"
  }
}
